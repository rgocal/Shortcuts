<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/backtemp"
        android:left="9dp"
        android:right="8dp"
        android:top="8dp"
        android:bottom="9dp">
        <shape
            android:shape="oval" >
            <solid android:color="@color/default_color"/>
            <corners
                android:bottomLeftRadius="25dp"
                android:topLeftRadius="25dp"
                android:topRightRadius="25dp"
                android:bottomRightRadius="9dp"/>
        </shape>
    </item>

    <item
        android:id="@+id/backtemp2"
        android:left="70dp"
        android:right="8dp"
        android:top="70dp"
        android:bottom="8dp">
        <shape
            android:shape="rectangle" >
            <solid android:color="@color/default_color"/>
            <corners
                android:bottomLeftRadius="25dp"
                android:topLeftRadius="25dp"
                android:topRightRadius="25dp"
                android:bottomRightRadius="14dp"/>
        </shape>
    </item>

    <item>
        <bitmap
            android:gravity="center"
            android:src="@drawable/ic_launcher_trans"/>
    </item>
</layer-list>
